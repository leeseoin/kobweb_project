{
  "name": "kobweb-fullstack",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd kobweb_back && gradlew.bat bootRun",
    "dev:frontend": "cd kobweb_pront && npm run dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd kobweb_back && gradlew.bat build",
    "build:frontend": "cd kobweb_pront && npm run build",
    "install:frontend": "cd kobweb_pront && npm install",
    "setup": "npm run install:frontend",
    "clean": "npm run clean:backend && npm run clean:frontend",
    "clean:backend": "cd kobweb_back && gradlew.bat clean",
    "clean:frontend": "cd kobweb_pront && rm -rf .next node_modules/.cache"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}